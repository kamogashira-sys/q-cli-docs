  "sources": {
    "source_code": {
      "priority": 1,
      "description": "Q CLIソースコードから抽出",
      "paths": {
        "agent_dir": "~/.aws/amazonq/cli-agents",
        "subagents_dir": ".amazonq/.subagents",
        "mcp_config": "~/.aws/amazonq/mcp.json",
        "global_config": "~/.aws/amazonq/config.toml",
        "knowledge_dir": "~/.aws/amazonq/knowledge",
        "prompts_dir": "~/.aws/amazonq/prompts"
      },
      "env_vars": {
        "Q_LOG_LEVEL": {
          "default": "info",
          "type": "string",
          "description": "ログレベル"
        },
        "Q_TELEMETRY_ENABLED": {
          "default": "true",
          "type": "boolean",
          "description": "テレメトリー有効化"
        },
        "Q_AGENT_NAME": {
          "default": null,
          "type": "string",
          "description": "Agent名"
        },
        "Q_AGENT_PATH": {
          "default": null,
          "type": "string",
          "description": "Agentパス"
        },
        "Q_MCP_CONFIG_PATH": {
          "default": "~/.aws/amazonq/mcp.json",
          "type": "string",
          "description": "MCP設定ファイルパス"
        },
        "Q_AGENT_CONFIG": {
          "default": null,
          "type": "string",
          "description": "Agent設定ファイルのパスを環境変数で指定"
        },
        "Q_CHAT_MODEL": {
          "default": "anthropic.claude-3-5-sonnet-20241022-v2:0",
          "type": "string",
          "description": "チャットで使用するモデルを指定"
        },
        "Q_OAUTH_REDIRECT_URI": {
          "default": null,
          "type": "string",
          "description": "OAuthフローで使用するリダイレクトURIを指定"
        },
        "Q_LOG_STDOUT": {
          "default": null,
          "type": "string",
          "description": "標準出力へのログ出力を有効化（値は問わない）"
        },
        "Q_DISABLE_TELEMETRY": {
          "default": null,
          "type": "string",
          "description": "テレメトリを無効化（値は問わない）"
        },
        "Q_SHELL": {
          "default": null,
          "type": "string",
          "description": "qtermで使用するシェル"
        },
        "Q_ZDOTDIR": {
          "default": null,
          "type": "string",
          "description": "ZDOTDIR環境変数をオーバーライド"
        },
        "Q_DEBUG_SHELL": {
          "default": null,
          "type": "string",
          "description": "シェルデバッグモードを有効化（値は問わない）"
        },
        "Q_FAKE_IS_REMOTE": {
          "default": null,
          "type": "string",
          "description": "リモート環境をシミュレート（テスト用、値は問わない）"
        },
        "Q_CODESPACES": {
          "default": null,
          "type": "string",
          "description": "Codespaces環境を示す（値は問わない）"
        },
        "Q_CI": {
          "default": null,
          "type": "string",
          "description": "CI環境を示す（値は問わない）"
        },
        "Q_BUNDLE_METADATA_PATH": {
          "default": null,
          "type": "string",
          "description": "バンドルメタデータへのパスをオーバーライド"
        },
        "Q_MAX_CONTEXT_TOKENS": {
          "default": null,
          "type": "string",
          "description": "最大コンテキストトークン数を指定"
        },
        "Q_ENABLE_THINKING": {
          "default": null,
          "type": "string",
          "description": "Thinking機能を有効化"
        },
        "Q_KNOWLEDGE_ENABLED": {
          "default": "true",
          "type": "string",
          "description": "Knowledge機能を有効化"
        },
        "Q_DEBUG": {
          "default": null,
          "type": "string",
          "description": "デバッグモードを有効化"
        },
        "Q_CLI_CLIENT_APPLICATION": {
          "default": null,
          "type": "string",
          "description": "クライアントアプリケーションの識別子"
        },
        "Q_DISABLE_TRUECOLOR": {
          "default": null,
          "type": "string",
          "description": "Trueカラー表示を無効化"
        },
        "Q_MOCK_CHAT_RESPONSE": {
          "default": null,
          "type": "string",
          "description": "チャット応答をモック（テスト用）"
        },
        "Q_AGENT": {
          "default": null,
          "type": "string",
          "description": "Agent名を環境変数で指定"
        }
